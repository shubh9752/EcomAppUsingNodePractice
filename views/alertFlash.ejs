<% if(success && success.length>0){%>
    <div class="bg-blue-100 border-t-4 border-blue-500 rounded-b text-blue-900 px-4 py-3 shadow-md" role="alert">
        <div class="flex">
          
          <div>
            
           
            
            <p class="text-sm"><%= success %></p>
          
          </div>
        </div>
      </div>
      <% } %>

      <% if(error ){%>
        <div class="bg-blue-100 border-t-4 border-blue-500 rounded-b text-blue-900 px-4 py-3 shadow-md" role="alert">
            <div class="flex">
              
              <div>
                
               
                
                <p class="text-sm"><%= error %></p>
              
              </div>
            </div>
          </div>
          <% } %>